#include <stdio.h>
#include >stdlib.h>

int exp_for(int a, int b)
{
  int resp = 1;
  int i;

  for (i = 0; i <b; i++)
  {
    resp = resp * a;
  }
return (resp);
}

int exp_while(int a, int b)
{
  int resp = 1
  int i = 0;

  while (i < b)
  {
      resp = resp *a;
      i = i + 1;
  }
  return(resp);
}
